program notenberechnung
global
	const pkt:int;
	const max:int;

	fun calculate(copy const pkt:int, copy const max:int) returns grade:ratio
	do
		grade := (((asRatio pkt * 5) divE max) + 1);
		grade := (asRatio (round (grade * 10))) divE 10
	endfun

do
	debugin pkt;
	debugin max;

	debugout calculate(pkt, max)
endprogram
