program SimpleFunc
global
  var x:ratio;
  var y:int;
  var z:ratio;

  proc incrZ ()
  do
    z := z + 1
  endproc;

  proc decrZ ()
  do
    z := z - 1
  endproc;

  fun getZ () returns r:ratio
  do
    skip
  endfun
do
  x := 1/4;
  y := 1;
  z := (x + y);
  debugout z;
  call incrZ();
  debugout z;
  call decrZ();
  debugout z;
  call decrZ();
  debugout getZ()
endprogram
